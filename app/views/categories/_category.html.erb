<div id="<%= dom_id category %>" >
  <div class="card p-2">
  <%= turbo_frame_tag "cat_edit_#{category.id}" do %>
    <%= category.name %>
    <%= link_to edit_category_path(category.id), class: 'grey_link' do %>
        <i class="fa-solid fa-pen-to-square"></i>
    <% end %>
  <% end %>
  </div>
</div>